@yearly		echo "Tous les ans			0 0 1 1 *"
@annually	echo "== @yearly"
@monthly	echo "Tous les mois			0 0 1 * *"
@weekly		echo "Toutes les semaines	0 0 * * 0"
@daily		echo "Tous les jours		0 0 * * *"
@midnight	echo "== @daily"
@hourly		echo "Toutes les heures		0 * * * *"

